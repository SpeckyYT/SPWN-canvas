import './src'

let myCanvas = @canvas::new(
    x = 40,
    y = 20,
    width = 31,
    height = 31,
    scaling = 10/31,
    use_groups = false,
)

myCanvas.setColor(rgb8(0,0,0))
myCanvas.drawFill()

myCanvas.setColor(rgb8(255,0,0))
myCanvas.drawPixel(1,1)

myCanvas.setColor(rgb8(255,0,128))
myCanvas.drawRect(2,3,4,3)

myCanvas.setColor(rgb8(255,0,255))
myCanvas.drawCircle(6,6,24)
